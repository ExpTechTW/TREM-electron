{
  "name": "trem",
  "version": "5.1.4-45-pre6",
  "description": "Taiwan Real-time Earthquake Monitoring ( 臺灣即時地震監測 )",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "dist": "electron-builder",
    "lint": "eslint --ext .js js/"
  },
  "build": {
    "productName": "TREM",
    "appId": "tw.exptech.trem",
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": "github",
      "target": [],
      "icon": "TREM.ico"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dev"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpTechTW/TREM.git"
  },
  "keywords": [],
  "author": "whes1015",
  "license": "AGPL-3.0",
  "devDependencies": {
    "electron": "^18.3.15",
    "electron-builder": "^23.0.3",
    "eslint": "^8.23.1",
    "eslint-plugin-require-sort": "^1.2.2"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@kamiya4047/exptech-api-wrapper": "^1.0.4",
    "@material/material-color-utilities": "^0.2.0",
    "axios": "^1.0.0",
    "bytenode": "^1.3.6",
    "chokidar": "^3.5.3",
    "discord.js": "^13.8.1",
    "electron-common-ipc": "^15.0.3",
    "electron-fcm-push-receiver": "^2.1.7",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "fs": "^0.0.1-security",
    "jquery": "^3.6.0",
    "leaflet": "^1.8.0",
    "leaflet-edgebuffer": "^1.0.6",
    "leaflet-geojson-vt": "^1.1.0",
    "lodash": "^4.17.21",
    "tinycolor2": "^1.4.2",
    "ws": "^8.8.0"
  }
}
