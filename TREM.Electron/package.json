{
  "name": "trem",
  "version": "6.0.0-beta.10",
  "description": "Taiwan Real-time Earthquake Monitoring ( 臺灣即時地震監測 )",
  "main": "main.js",
  "homepage": "https://exptech.com.tw/",
  "author": "ExpTechTW <ExpTech.tw@gmail.com>",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "dist": "electron-builder",
    "lint": "eslint --ext .js js/"
  },
  "build": {
    "productName": "TREM",
    "appId": "tw.exptech.trem",
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": null,
      "target": [],
      "icon": "TREM.ico"
    },
    "linux": {
      "publish": null,
      "target": "deb",
      "category": "Development",
      "maintainer": "ExpTechTW"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dev"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpTechTW/TREM.git"
  },
  "keywords": [],
  "license": "AGPL-3.0",
  "devDependencies": {
    "@parcel/transformer-pug": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "electron": "22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.28.0",
    "parcel": "^2.8.3"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@kamiya4047/exptech-api-wrapper": "^2.0.1",
    "@material/material-color-utilities": "^0.2.3",
    "bytenode": "^1.3.7",
    "chokidar": "^3.5.3",
    "chroma-js": "^2.4.2",
    "electron-common-ipc": "^15.0.3",
    "electron-fcm-push-receiver": "^2.1.7",
    "electron-log": "^4.4.8",
    "expose-gc": "^1.0.0",
    "jquery": "^3.6.1",
    "leaflet": "^1.9.3",
    "leaflet-edgebuffer": "^1.0.6",
    "maplibre-gl": "^2.4.0",
    "node-datachannel": "^0.4.0",
    "tinycolor2": "^1.4.2",
    "ws": "^8.11.0"
  }
}
